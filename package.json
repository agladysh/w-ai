{
  "name": "@w-ai/workspace",
  "version": "0.1.0",
  "private": true,
  "description": "W-AI: AI Must Work!",
  "scripts": {
    "pretest": "pnpm run lint",
    "test": "true",
    "lint:eslint": "eslint",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --noEmit",
    "lint:markdownlint": "markdownlint --ignore '**/node_modules/**/*' '**/*.md'",
    "lint": "pnpm run lint:tsc && pnpm run lint:eslint && pnpm run lint:prettier && pnpm run lint:markdownlint",
    "fix:eslint": "eslint --fix",
    "fix:prettier": "prettier --write .",
    "fix:markdownlint": "markdownlint --ignore '**/node_modules/**/*' --fix '**/*.md'",
    "fix": "pnpm run fix:eslint && pnpm run fix:prettier && pnpm run fix:markdownlint",
    "lefthook:precommit": "pnpm exec lefthook run pre-commit",
    "link": "pnpm uninstall -g @w-ai/cli || true && pnpm link --global \"$(pwd)/packages/cli\""
  },
  "keywords": [
    "w-ai"
  ],
  "author": "Alexander Gladysh <agladysh@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "@w-ai/cli": "workspace:*",
    "@w-ai/plugin-stub": "workspace:*"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "lefthook": "^1.12.3",
    "markdownlint-cli": "^0.45.0",
    "prettier": "^3.6.2",
    "tsc-files": "^1.1.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  }
}
